<?xml version="1.0"?>
<root>
  <list>
    <item>
      <name>[Key Repeat] Initial Wait</name>
      <sysctl>repeat.initial_wait</sysctl>
      <default>500</default>
      <baseunit>ms</baseunit>
    </item>
    <item>
      <name>[Key Repeat] Wait</name>
      <sysctl>repeat.wait</sysctl>
      <default>30</default>
      <baseunit>ms</baseunit>
    </item>
    <item>
      <name>[Key Overlaid Modifier] Timeout</name>
      <sysctl>parameter.keyoverlaidmodifier_timeout</sysctl>
      <default>1000</default>
      <baseunit>ms</baseunit>
    </item>
    <item>
      <name>[Key Overlaid Modifier's Key Repeat] Initial Wait</name>
      <sysctl>repeat.keyoverlaidmodifier_initial_wait</sysctl>
      <default>500</default>
      <baseunit>ms</baseunit>
    </item>
    <item>
      <name>[Modifier Holding Key to Key] Holding Threshold</name>
      <sysctl>parameter.modifierholdingkeytokey_wait</sysctl>
      <default>1000</default>
      <baseunit>ms</baseunit>
    </item>
    <item>
      <name>[Pointing Device] CursorMove to ScrollWheel Rate</name>
      <sysctl>pointing.relative2scroll_rate</sysctl>
      <default>150</default>
      <baseunit>/ 1000</baseunit>
    </item>
    <item>
      <name>[StatusWindow] AlphaValue for Font</name>
      <sysctl>parameter.statuswindow_alpha_font</sysctl>
      <default>60</default>
      <baseunit>/ 100</baseunit>
    </item>
    <item>
      <name>[StatusWindow] AlphaValue for Background</name>
      <sysctl>parameter.statuswindow_alpha_background</sysctl>
      <default>20</default>
      <baseunit>/ 100</baseunit>
    </item>
  </list>
</root>
