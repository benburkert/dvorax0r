<?xml version="1.0"?>
<root>
  <list>
    <item>
      <name>[Key Repeat] Initial Wait</name>
      <sysctl>keyremap4macbook.repeat.initial_wait</sysctl>
      <default>500</default>
      <baseunit>ms</baseunit>
    </item>
    <item>
      <name>[Key Repeat] Wait</name>
      <sysctl>keyremap4macbook.repeat.wait</sysctl>
      <default>30</default>
      <baseunit>ms</baseunit>
    </item>
    <item>
      <name>[Pointing Device] CursorMove to ScrollWheel Ratio</name>
      <sysctl>keyremap4macbook.pointing.relative2scroll_ratio</sysctl>
      <default>750</default>
      <baseunit>/1000</baseunit>
    </item>
    <item>
      <name>[Pointing Device] CursorMove to ScrollWheel Fixed Coefficient</name>
      <sysctl>keyremap4macbook.pointing.relative2scroll_fixed_coefficient</sysctl>
      <default>6554</default>
      <baseunit></baseunit>
    </item>
    <item>
      <name>[Pointing Device] CursorMove to ScrollWheel Pointing Coefficient</name>
      <sysctl>keyremap4macbook.pointing.relative2scroll_point_coefficient</sysctl>
      <default>10</default>
      <baseunit></baseunit>
    </item>
  </list>
</root>
